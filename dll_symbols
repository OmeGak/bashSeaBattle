tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo " #####"; row=$(($row + 1))
tput cup $row $colum; echo "     #"; row=$(($row + 1))
tput cup $row $colum; echo "     #"; row=$(($row + 1))
tput cup $row $colum; echo " ####";;

#! /bin/bash

ShowLetter()
{
	letter=$1
	startRow=$2
	row=$2
	colum=$3
	SetColor $4

	case $letter in
		( "(" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #" 		; row=$((row + 1))
			gotoXY $row $colum; echo " #"  		; row=$((row + 1))
			gotoXY $row $colum; echo " #"  		; row=$((row + 1))
			gotoXY $row $colum; echo " #"	 	; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #" 		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		;;
		( ")" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#"  		; row=$((row + 1))
			gotoXY $row $colum; echo " #" 		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #" 		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		( "!" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		;;
		( "." )
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		;;
		( "," )
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		;;
		( "1" )
			gotoXY $row $colum; echo "    # " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   ## " 	; row=$((row + 1))
			gotoXY $row $colum; echo "  # # " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    # " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   ###"	;;
		( "2" )
			gotoXY $row $colum; echo " ####  " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    # " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    #  " 	; row=$((row + 1))
			gotoXY $row $colum; echo "  #    " 	; row=$((row + 1))
			gotoXY $row $colum; echo " ##### "	;;
		( "3" )
			gotoXY $row $colum; echo " #### "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   ## " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### "	;;
		( "4" )
			gotoXY $row $colum; echo "    # " 	; row=$((row + 1))
			gotoXY $row $colum; echo "   ## " 	; row=$((row + 1))
			gotoXY $row $colum; echo "  # # " 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "    # "	;;
		( "5" )
			gotoXY $row $colum; echo "######" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#     " 	; row=$((row + 1))
			gotoXY $row $colum; echo "##### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "##### "	;;
		( "6" )
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#     " 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### "	;;
		( "7" )
			gotoXY $row $colum; echo "#####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "   # " 	; row=$((row + 1))
			gotoXY $row $colum; echo " ### " 	; row=$((row + 1))
			gotoXY $row $colum; echo " #   " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	;;
		( "8" )
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### "	;;
		( "9" )
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #### "	;;
		( "0" )
			gotoXY $row $colum; echo "  ### " 	; row=$((row + 1))
			gotoXY $row $colum; echo " #  ##" 	; row=$((row + 1))
			gotoXY $row $colum; echo " # # #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " ##  #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "  ### "	;;
		( "a" )
			gotoXY $row $colum; echo " ####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " ### #"	;;
		( "A" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "b" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	;;
		( "B" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	;;
		( "c" )
			gotoXY $row $colum; echo " #####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo " #####";;
		( "C" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " #####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo " #####"	;;
		( "d" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #####" 	;;
		( "D" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#####" 	;;
		( "e" )
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "E" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "######" 	;;
		( "f" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "  ####"	; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "####"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		;;
		( "F" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		("g")
			gotoXY $row $colum; echo " #####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #####"	; row=$((row + 1))
			gotoXY $row $colum; echo "     #"	; row=$((row + 1))
			gotoXY $row $colum; echo "     #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "G" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " #### "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#     "	; row=$((row + 1))
			gotoXY $row $colum; echo "#  ###"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #### " 	;;
		( "h" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "H" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	;;
		( "i" )
			row=$(($row - 2));	  colum=$(($colum + 2))
			gotoXY $row $colum; echo "#"		; row=$(($row + 2))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		( "I" )
			row=$(($row - 2));	  colum=$(($colum + 2))
			gotoXY $row $colum; echo "###"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "###"		;;
		("j")
			gotoXY $row $colum; echo "   #"		; row=$(($row + 2))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "###"		;;
		("J")
			row=$(($row - 2));
			gotoXY $row $colum; echo "   ###"	; row=$((row + 1))
			gotoXY $row $colum; echo "    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#   #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ###"		;;
		( "k" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "# #"		; row=$((row + 1))
			gotoXY $row $colum; echo "##"		; row=$((row + 1))
			gotoXY $row $colum; echo "# #"		; row=$((row + 1))
			gotoXY $row $colum; echo "#  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "#   #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "K" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#   #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "# #"		; row=$((row + 1))
			gotoXY $row $colum; echo "##"		; row=$((row + 1))
			gotoXY $row $colum; echo "#  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "#   #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "l" )
			row=$(($row - 3))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  ###"	;;
		( "L" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #####"	;;
		( "m" )
			gotoXY $row $colum; echo "######"  	; row=$((row + 1))
			gotoXY $row $colum; echo "#  #  #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#  #  #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#  #  #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#  #  #"	;;
		( "M" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "##  ##" 	; row=$((row + 1))
			gotoXY $row $colum; echo "# ## #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "n" )
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "N" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "##   #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "# #  #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#  # #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#   ##" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "o" )
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "O" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "p" )
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		( "P" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " #### " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    " 	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		( "q" )
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " #####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #" 	;;
		( "Q" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#   ##"	; row=$((row + 1))
			gotoXY $row $colum; echo " ### #"	;;
		( "r" )
			gotoXY $row $colum; echo "# ###"	; row=$((row + 1))
			gotoXY $row $colum; echo "##   #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		;;
		( "R" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "##### "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "##### "	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "s" )
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo " ####" 	; row=$((row + 1))
			gotoXY $row $colum; echo "     #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "S" )
			row=$(($row - 2))
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    "	; row=$((row + 1))
			gotoXY $row $colum; echo " #### "	; row=$((row + 1))
			gotoXY $row $colum; echo "     #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "t" )
			row=$(($row - 3))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   ###"	;;
		( "T" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		;;
		( "u" )
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #" 	; row=$((row + 1))
			gotoXY $row $colum; echo " ######"	;;
		( "U" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ####"	;;
		( "v" )
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ##" 	;;
		( "V" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ##"		;;
		( "w" )
			gotoXY $row $colum; echo "# # #"	; row=$((row + 1))
			gotoXY $row $colum; echo "# # #"	; row=$((row + 1))
			gotoXY $row $colum; echo "# # #"	; row=$((row + 1))
			gotoXY $row $colum; echo "# # #"	; row=$((row + 1))
			gotoXY $row $colum; echo " ###"		;;
		( "W" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "# ## #"	; row=$((row + 1))
			gotoXY $row $colum; echo "# ## #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	;;
		( "x" )
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ##"		; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		( "X" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ##"		; row=$((row + 1))
			gotoXY $row $colum; echo "  ##"		; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	;;
		("y")
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #####"	; row=$((row + 1))
			gotoXY $row $colum; echo "     #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #   #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ###"	;;
		( "Y" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "#    #"	; row=$((row + 1))
			gotoXY $row $colum; echo " #  #"	; row=$((row + 1))
			gotoXY $row $colum; echo "  ##"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		;;
		( "z" )
			gotoXY $row $colum; echo "#####"	; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "#####"	;;
		( "Z" )
			row=$(($row - 2))
			gotoXY $row $colum; echo "######"	; row=$((row + 1))
			gotoXY $row $colum; echo "    #"	; row=$((row + 1))
			gotoXY $row $colum; echo "   #"		; row=$((row + 1))
			gotoXY $row $colum; echo "  #"		; row=$((row + 1))
			gotoXY $row $colum; echo " #"		; row=$((row + 1))
			gotoXY $row $colum; echo "#"		; row=$((row + 1))
			gotoXY $row $colum; echo "######"	;;
	esac
	
	gotoXY $startRow $colum
}
########################
TypeWord()
{
	word=$1
	x=$2
	y=$3
	
	for ((i=1; i <= $(Length "$word"); i++ ))
	do 
		symbol=$(SubString "$word" $i 1)
		ShowLetter $symbol $x $y $4
		y=$((y + 7))
	done
}

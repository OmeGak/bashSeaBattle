#! /bin/bash

ShowLetter()
{
letter=$1
row=$2
colum=$3
SetColor $4

case $letter in
( "(" )
row=$(($row - 2))
tput cup $row $colum; echo "   #"; row=$(($row + 1))
tput cup $row $colum; echo "  #" ; row=$(($row + 1))
tput cup $row $colum; echo " #"  ; row=$(($row + 1))
tput cup $row $colum; echo " #"  ; row=$(($row + 1))
tput cup $row $colum; echo " #"	 ; row=$(($row + 1))
tput cup $row $colum; echo " #"	 ; row=$(($row + 1))
tput cup $row $colum; echo "  #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #";;
( ")" )
row=$(($row - 2))
tput cup $row $colum; echo "#"  ; row=$(($row + 1))
tput cup $row $colum; echo " #" ; row=$(($row + 1))
tput cup $row $colum; echo "  #"; row=$(($row + 1))
tput cup $row $colum; echo "  #"; row=$(($row + 1))
tput cup $row $colum; echo "  #"; row=$(($row + 1))
tput cup $row $colum; echo "  #"; row=$(($row + 1))
tput cup $row $colum; echo " #" ; row=$(($row + 1))
tput cup $row $colum; echo "#"	;;
( "!" )
row=$(($row - 3))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "   #";;
( "." )
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "   #";;
( "," )
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "    " ; row=$(($row + 1))
tput cup $row $colum; echo "   #" ; row=$(($row + 1))
tput cup $row $colum; echo "  #";;
( "1" )
tput cup $row $colum; echo "    # " ; row=$(($row + 1))
tput cup $row $colum; echo "   ## " ; row=$(($row + 1))
tput cup $row $colum; echo "  # # " ; row=$(($row + 1))
tput cup $row $colum; echo "    # " ; row=$(($row + 1))
tput cup $row $colum; echo "   ###";;
( "2" )
tput cup $row $colum; echo " ####  " ; row=$(($row + 1))
tput cup $row $colum; echo "#    # " ; row=$(($row + 1))
tput cup $row $colum; echo "    #  " ; row=$(($row + 1))
tput cup $row $colum; echo "  #    " ; row=$(($row + 1))
tput cup $row $colum; echo " ##### ";;
( "3" )
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "   ## " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### ";;
( "4" )
tput cup $row $colum; echo "    # " ; row=$(($row + 1))
tput cup $row $colum; echo "   ## " ; row=$(($row + 1))
tput cup $row $colum; echo "  # # " ; row=$(($row + 1))
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "    # ";;
( "5" )
tput cup $row $colum; echo "######" ; row=$(($row + 1))
tput cup $row $colum; echo "#     " ; row=$(($row + 1))
tput cup $row $colum; echo "##### " ; row=$(($row + 1))
tput cup $row $colum; echo "     #" ; row=$(($row + 1))
tput cup $row $colum; echo "##### ";;
( "6" )
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#     " ; row=$(($row + 1))
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### ";;
( "7" )
tput cup $row $colum; echo "#####" ; row=$(($row + 1))
tput cup $row $colum; echo "   # " ; row=$(($row + 1))
tput cup $row $colum; echo " ### " ; row=$(($row + 1))
tput cup $row $colum; echo " #   " ; row=$(($row + 1))
tput cup $row $colum; echo "#    ";;
( "8" )
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### ";;
( "9" )
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### " ; row=$(($row + 1))
tput cup $row $colum; echo "     #" ; row=$(($row + 1))
tput cup $row $colum; echo " #### ";;
( "0" )
tput cup $row $colum; echo "  ### " ; row=$(($row + 1))
tput cup $row $colum; echo " #  ##" ; row=$(($row + 1))
tput cup $row $colum; echo " # # #" ; row=$(($row + 1))
tput cup $row $colum; echo " ##  #" ; row=$(($row + 1))
tput cup $row $colum; echo "  ### ";;
( "a" )
tput cup $row $colum; echo " ####" ; row=$(($row + 1))
tput cup $row $colum; echo "#   #" ; row=$(($row + 1))
tput cup $row $colum; echo "#   #" ; row=$(($row + 1))
tput cup $row $colum; echo "#   #" ; row=$(($row + 1))
tput cup $row $colum; echo " ### #";;
( "A" )
row=$(($row - 2))
tput cup $row $colum; echo " ####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "######"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #";;
( "b" )
row=$(($row - 3))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#####"	;;
( "B" )
row=$(($row - 2))
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#####"	;;
( "c" )
tput cup $row $colum; echo " #####"	;	row=$(($row + 1))
tput cup $row $colum; echo "#"		;	row=$(($row + 1))
tput cup $row $colum; echo "#"		;	row=$(($row + 1))
tput cup $row $colum; echo "#"		;	row=$(($row + 1))
tput cup $row $colum; echo " #####";;
( "C" )
row=$(($row - 2))
tput cup $row $colum; echo " #####"	; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo " #####"	;;
( "d" )
row=$(($row - 3))
tput cup $row $colum; echo "     #" ; row=$(($row + 1))
tput cup $row $colum; echo "     #" ; row=$(($row + 1))
tput cup $row $colum; echo "     #" ; row=$(($row + 1))
tput cup $row $colum; echo " #####" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " #####" ;;
( "D" )
row=$(($row - 2))
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#####" ;;

tput cup $row $colum; echo "##### "	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	;;
( "s" )
tput cup $row $colum; echo " ####"	; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo " ####" 	; row=$(($row + 1))
tput cup $row $colum; echo "     #"	; row=$(($row + 1))
tput cup $row $colum; echo " ####"	;;
( "S" )
row=$(($row - 2))
tput cup $row $colum; echo " ####"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    "	; row=$(($row + 1))
tput cup $row $colum; echo " #### "	; row=$(($row + 1))
tput cup $row $colum; echo "     #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " ####"	;;
( "t" )
row=$(($row - 3))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo " ####"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "   ###"	;;
( "T" )
row=$(($row - 2))
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	;;
( "u" )
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo "#    #" ; row=$(($row + 1))
tput cup $row $colum; echo " ######";;
( "U" )
row=$(($row - 2))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " ####"	;;
( "v" )
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo "#    #"; row=$(($row + 1))
tput cup $row $colum; echo " #  #" ; row=$(($row + 1))
tput cup $row $colum; echo "  ##" ;;
( "V" )
row=$(($row - 2))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  ##"	;;
( "w" )
tput cup $row $colum; echo "# # #"; row=$(($row + 1))
tput cup $row $colum; echo "# # #"; row=$(($row + 1))
tput cup $row $colum; echo "# # #"; row=$(($row + 1))
tput cup $row $colum; echo "# # #"; row=$(($row + 1))
tput cup $row $colum; echo " ###";;
( "W" )
row=$(($row - 2))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "# ## #"	; row=$(($row + 1))
tput cup $row $colum; echo "# ## #"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	;;
( "x" )
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  ##"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #";;
( "X" )
row=$(($row - 2))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  ##"	; row=$(($row + 1))
tput cup $row $colum; echo "  ##"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	;;
("y")
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " #####"	; row=$(($row + 1))
tput cup $row $colum; echo "     #"	; row=$(($row + 1))
tput cup $row $colum; echo " #   #"	; row=$(($row + 1))
tput cup $row $colum; echo "  ###"	;;
( "Y" )
row=$(($row - 2))
tput cup $row $colum; echo "#    #"	; row=$(($row + 1))
tput cup $row $colum; echo " #  #"	; row=$(($row + 1))
tput cup $row $colum; echo "  ##"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	;;
( "z" )
tput cup $row $colum; echo "#####"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo " #"		; row=$(($row + 1))
tput cup $row $colum; echo "#####"	;;
( "Z" )
row=$(($row - 2))
tput cup $row $colum; echo "######"	; row=$(($row + 1))
tput cup $row $colum; echo "    #"	; row=$(($row + 1))
tput cup $row $colum; echo "   #"	; row=$(($row + 1))
tput cup $row $colum; echo "  #"	; row=$(($row + 1))
tput cup $row $colum; echo " # "	; row=$(($row + 1))
tput cup $row $colum; echo "#"		; row=$(($row + 1))
tput cup $row $colum; echo "######"	;;
#abcdefghijklmnopqrst uvwxyz
esac
tput cup $(($2+9)) 1
}
########################
TypeWord()
{
word=$1
x=$2; y=$3

for ((i=1; i <= `echo "$word" | awk '{print length($0)}'`; i++ ))
do 
	symbol=`echo "$word" $i | awk '{print substr($1, $2, 1)}'`
	ShowLetter $symbol $x $y $4
	y=$((y + 7))
done
}
